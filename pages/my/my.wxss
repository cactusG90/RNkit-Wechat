page{
  font-size: 14px;
  margin-bottom: 50px;
}
.myInfo{
  text-align:center;
  background: #f6f6f6;
  padding: 20px 0;
}

.myInfo .logo{
  display: block;
  width: 50px;
  height: 50px;
  padding:5px;
  border-radius: 50%;
  border:1px solid #20a0ff;
  margin: 10px auto;
}
.myMsg text{
  display:inline-block;
  width:100%;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
  font-size:14px;
}
.editInfo{
  margin: 10px 0;
}
.editInfo navigator{
  display: block;
  width: 100%;
  height: 35px;
  padding: 0 5px;
  line-height: 35px;
  border-top: 1px solid #f5f5f5;
}
.editInfo navigator:last-child{
  border-bottom: 1px solid #f5f5f5;
}
.logOut{
  width: 100%;
  height: 40px;
  line-height: 40px;
  font-size:14px;
  text-align: center;
  border: 1px solid #f5f5f5;
}
.footer{
  width:100%;
  height: 50px;
  position: fixed;
  bottom: 0;
  font-size:14px;
  background:#fff;
  color:#707070;
  border-top: 1px solid #eee;
}
.footer view{
  display: inline-block;
  width: 50%;
  text-align: center;
}
.footer .list::before, .footer .my::before{
  content: ' ';
  display: block;
  width:20px;
  height: 20px;
  margin: 5px auto;
}
.footer .list::before{
  background: url(../../img/list.svg) no-repeat;
  background-size: contain;
}
.footer .checked {
  color:#20a0ff;
}
.footer .my::before {
  background: url(../../img/my2.svg) no-repeat;
  background-size: contain;
}

