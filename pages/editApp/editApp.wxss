page{
  font-size: 14px;
}
.appInfo{
  width: 100%;
  height: 30vh;
  overflow: hidden;
  text-align:center;
  background: #f4f4f4;
}
.appInfo .logo{
  display: block;
  width: 50px;
  height: 30%;
  margin: 5px auto;
}
.appMsg text{
  display:inline-block;
  width:100%;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
  font-size:14px;
}
.appMsg .app-name{
  font-size: 16px;
  font-weight: bolder;
}
.appMsg .platformTxt{
  width: initial;
  vertical-align:middle;
}
.appMsg .platform image{
  width:14px;
  height:14px;
  margin-left:5px;
}
.editApp{
  padding:10px;
}
.editApp text{
  display: block;
  line-height:30px;
}
.editApp .editInput{
  display: block;
  height: 30px;
  margin-bottom: 10px
}
.editApp .editInput text{
  float: left;
}
.editApp .editInput input{
  float: left;
  width: 75%;
  padding: 0 5px;
  border: 1px solid #eee;
}
.editApp button{
  width: 85%;
  margin: 20px auto 0;
  background: #20a0ff;
}
button[disabled][type="primary"] {
  background-color:#20a0ff;
  opacity: 0.7
}
.button-hover[type="primary"] {
  background-color:#20a0ff;
}


